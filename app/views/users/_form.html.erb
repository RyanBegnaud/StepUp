<%= form_for @user do |f| %> 
    <%= f.label "First Name:" %>
    <%= f.text_field :first_name %><br><br>
    <%= f.label "Last Name:" %>
    <%= f.text_field :last_name %><br><br>
    <%= f.label :username %> 
    <%= f.text_field :username %><br><br>
    <%= f.label :password %> 
    <%= f.text_field :password %><br><br>
<%= f.submit "Sign Up" %>
<% end %> 